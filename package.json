{
  "name": "silkky.cloud",
  "version": "0.1.2",
  "description": "Source code for silkky.cloud",
  "main": "server.js",
  "scripts": {
    "test": "exit 1",
    "start": "node server.js",
    "css": "npm-run-all css-compile css-prefix css-minify",
    "css-compile": "sass --style expanded --source-map --embed-sources --no-error-css scss/styles.scss:public/css/styles.css",
    "css-prefix": "postcss public/css/styles.css --output public/css/styles.css",
    "css-minify": "cleancss public/css/styles.css --format breakWith=lf --with-rebase --source-map --source-map-inline-sources --output public/css/styles.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheSilkky/silkky.cloud.git"
  },
  "keywords": [
    "website"
  ],
  "author": "TheSilkky",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/TheSilkky/silkky.cloud/issues"
  },
  "homepage": "https://github.com/TheSilkky/silkky.cloud",
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "autoprefixer": "^10.2.6",
    "bootstrap": "^5.0.2",
    "clean-css": "^5.1.3",
    "clean-css-cli": "^5.3.0",
    "fastify": "^3.18.1",
    "fastify-helmet": "^5.3.1",
    "fastify-static": "^4.2.2",
    "middie": "^5.3.0",
    "npm-run-all": "^4.1.5",
    "pm2": "^5.1.0",
    "point-of-view": "^4.15.0",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "pug": "^3.0.2",
    "sass": "^1.35.2",
    "serve-static": "^1.14.1",
    "upath": "^2.0.1"
  }
}
